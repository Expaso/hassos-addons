{
  "name": "pgAdmin4",
  "version": "0.1.1",
  "stage": "experimental",
  "slug": "pgadmin4",
  "description": "A PostgreSQL Management and Query tool",
  "url": "https://github.com/Expaso/hassos-addon-pgadmin4",
  "arch": ["aarch64", "amd64", "armhf", "armv7", "i386"],
  "startup": "services",
  "ingress": true,
  "ingress_port": 80,
  "panel_icon": "mdi:database",
  "panel_title": "pgAdmin 4",
  "hassio_api": true,
  "hassio_role": "default",
  "boot": "auto",
  "init": false,
  "timeout": 20,
  "ports": {
	"80/tcp": null,
	"443/tcp": null
  },
  "ports_description": {
    "80/tcp": "Web interface (Not required for Ingress)",
    "443/tcp": "Web interface on SSL (Not required for Ingress)"
  },
  "map": ["ssl"],
  "options": {
    "ssl": true,
    "certfile": "fullchain.pem",
    "keyfile": "privkey.pem",
    "default_email": "",
    "default_password": ""
  },
  "schema": {
    "certfile": "str",
    "keyfile": "str",
    "ssl": "bool",
    "default_email": "str?",
    "default_password": "str?"
  },
  "image": "husselhans/hassos-addon-pgadmin4-{arch}"
}
